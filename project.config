[project]
	description = The DCI agent
[access "refs/*"]
  read = group dci-agent-core
  owner = group dci-agent-ptl
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group dci-agent-core
	label-Code-Review = -2..+2 group dci-agent-ptl
	label-Verified = -2..+2 group dci-agent-ptl
	label-Workflow = -1..+1 group dci-agent-core
	label-Workflow = -1..+1 group dci-agent-ptl
	label-Workflow = -1..+0 group Registered Users
	submit = group dci-agent-ptl
	read = group Registered Users
    read = group dci-agent-core
[access "refs/meta/config"]
    read = group Registered Users
    read = group dci-agent-core
[receive]
	requireChangeId = true
[submit]
	mergeContent = false
	action = fast forward only
